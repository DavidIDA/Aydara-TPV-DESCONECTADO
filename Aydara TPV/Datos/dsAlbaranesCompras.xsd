                         REFERENCIAS_PROVEEDORES.PrecioCoste AS PrecioCosteProveedor
FROM            ARTICULOS LEFT OUTER JOIN
                         REFERENCIAS_PROVEEDORES ON ARTICULOS.Id = REFERENCIAS_PROVEEDORES.Articulo_Id AND REFERENCIAS_PROVEEDORES.CodigoProveedor = @CodigoProveedor LEFT OUTER JOIN
                         EXISTENCIAS ON ARTICULOS.Id = EXISTENCIAS.Articulo_Id
GROUP BY ARTICULOS.Codigo, REFERENCIAS_PROVEEDORES.CodigoProveedor, REFERENCIAS_PROVEEDORES.Referencia, ARTICULOS.Descripcion, ARTICULOS.UnidadCaja, ARTICULOS.MinimoVenta, 
