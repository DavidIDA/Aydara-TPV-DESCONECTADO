                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        ARTICULOS.Codigo, REFERENCIAS_PROVEEDORES.CodigoProveedor, REFERENCIAS_PROVEEDORES.Referencia,
                             (SELECT        TOP (1) ALBARANES_COMPRAS.NumeroAlbaran
                               FROM            ALBARANES_COMPRAS INNER JOIN
                                                         ALBARANES_COMPRAS_LINEAS ON ALBARANES_COMPRAS.Id = ALBARANES_COMPRAS_LINEAS.AlbaranCompra_Id
                               WHERE        (ALBARANES_COMPRAS.CodigoProveedor = REFERENCIAS_PROVEEDORES.CodigoProveedor) AND (ALBARANES_COMPRAS_LINEAS.CodigoArticulo = ARTICULOS.Codigo)
                               ORDER BY ALBARANES_COMPRAS.Fecha DESC, ALBARANES_COMPRAS.Id DESC) AS Compra_Albaran,
                             (SELECT        TOP (1) ALBARANES_COMPRAS_3.Fecha
                               FROM            ALBARANES_COMPRAS AS ALBARANES_COMPRAS_3 INNER JOIN
                                                         ALBARANES_COMPRAS_LINEAS AS ALBARANES_COMPRAS_LINEAS_3 ON ALBARANES_COMPRAS_3.Id = ALBARANES_COMPRAS_LINEAS_3.AlbaranCompra_Id
                               WHERE        (ALBARANES_COMPRAS_3.CodigoProveedor = REFERENCIAS_PROVEEDORES.CodigoProveedor) AND (ALBARANES_COMPRAS_LINEAS_3.CodigoArticulo = ARTICULOS.Codigo)
                               ORDER BY ALBARANES_COMPRAS_3.Fecha DESC, ALBARANES_COMPRAS_3.Id DESC) AS Compra_Fecha,
                             (SELECT        TOP (1) ALBARANES_COMPRAS_LINEAS_2.Cantidad
                               FROM            ALBARANES_COMPRAS AS ALBARANES_COMPRAS_2 INNER JOIN
                                                         ALBARANES_COMPRAS_LINEAS AS ALBARANES_COMPRAS_LINEAS_2 ON ALBARANES_COMPRAS_2.Id = ALBARANES_COMPRAS_LINEAS_2.AlbaranCompra_Id
                               WHERE        (ALBARANES_COMPRAS_2.CodigoProveedor = REFERENCIAS_PROVEEDORES.CodigoProveedor) AND (ALBARANES_COMPRAS_LINEAS_2.CodigoArticulo = ARTICULOS.Codigo)
                               ORDER BY ALBARANES_COMPRAS_2.Fecha DESC, ALBARANES_COMPRAS_2.Id DESC, ALBARANES_COMPRAS_LINEAS_2.Id DESC) AS Compra_Unidades,
                             (SELECT        TOP (1) ALBARANES_COMPRAS_LINEAS_1.Precio
                               FROM            ALBARANES_COMPRAS AS ALBARANES_COMPRAS_1 INNER JOIN
                                                         ALBARANES_COMPRAS_LINEAS AS ALBARANES_COMPRAS_LINEAS_1 ON ALBARANES_COMPRAS_1.Id = ALBARANES_COMPRAS_LINEAS_1.AlbaranCompra_Id
                               WHERE        (ALBARANES_COMPRAS_1.CodigoProveedor = REFERENCIAS_PROVEEDORES.CodigoProveedor) AND (ALBARANES_COMPRAS_LINEAS_1.CodigoArticulo = ARTICULOS.Codigo)
                               ORDER BY ALBARANES_COMPRAS_1.Fecha DESC, ALBARANES_COMPRAS_1.Id DESC, ALBARANES_COMPRAS_LINEAS_1.Id DESC) AS Compra_Precio, ARTICULOS.Descripcion, ARTICULOS.UnidadCaja, 
