                       LEFT OUTER JOIN ARTICULOS ON ARTICULOS.Id = REFERENCIAS_PROVEEDORES.Articulo_Id
WHERE        (REFERENCIAS_PROVEEDORES.Articulo_Id = @IdArticulo)</CommandText>
