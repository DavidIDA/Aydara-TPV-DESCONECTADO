                               FROM            ALBARANES_COMPRAS INNER JOIN ALBARANES_COMPRAS_LINEAS ON ALBARANES_COMPRAS.Id = ALBARANES_COMPRAS_LINEAS.AlbaranCompra_Id
                               WHERE        (ALBARANES_COMPRAS.CodigoProveedor = REFERENCIAS_PROVEEDORES.CodigoProveedor) AND 
                                                         (ALBARANES_COMPRAS_LINEAS.CodigoArticulo = ARTICULOS.Codigo)
                               ORDER BY ALBARANES_COMPRAS.Fecha DESC, ALBARANES_COMPRAS.Id DESC) AS Compra_Albaran,
                             (SELECT        TOP (1) ALBARANES_COMPRAS.Fecha
                               FROM            ALBARANES_COMPRAS INNER JOIN ALBARANES_COMPRAS_LINEAS ON ALBARANES_COMPRAS.Id = ALBARANES_COMPRAS_LINEAS.AlbaranCompra_Id
                               WHERE        (CodigoProveedor = REFERENCIAS_PROVEEDORES.CodigoProveedor) AND 
                                                         (ALBARANES_COMPRAS_LINEAS.CodigoArticulo = ARTICULOS.Codigo)
                               ORDER BY ALBARANES_COMPRAS.Fecha DESC, ALBARANES_COMPRAS.Id DESC) AS Compra_Fecha,
                             (SELECT        TOP (1) ALBARANES_COMPRAS_LINEAS.Cantidad
                               FROM            ALBARANES_COMPRAS INNER JOIN ALBARANES_COMPRAS_LINEAS ON ALBARANES_COMPRAS.Id = ALBARANES_COMPRAS_LINEAS.AlbaranCompra_Id
                               WHERE        (CodigoProveedor = REFERENCIAS_PROVEEDORES.CodigoProveedor) AND 
                                                         (ALBARANES_COMPRAS_LINEAS.CodigoArticulo = ARTICULOS.Codigo)
                               ORDER BY ALBARANES_COMPRAS.Fecha DESC, ALBARANES_COMPRAS.Id DESC, ALBARANES_COMPRAS_LINEAS.Id DESC) AS Compra_Unidades,
                             (SELECT        TOP (1) ALBARANES_COMPRAS_LINEAS.Precio
                               FROM            ALBARANES_COMPRAS INNER JOIN ALBARANES_COMPRAS_LINEAS ON ALBARANES_COMPRAS.Id = ALBARANES_COMPRAS_LINEAS.AlbaranCompra_Id
                               WHERE        (CodigoProveedor = REFERENCIAS_PROVEEDORES.CodigoProveedor) AND 
                                                         (ALBARANES_COMPRAS_LINEAS.CodigoArticulo = ARTICULOS.Codigo)
                               ORDER BY ALBARANES_COMPRAS.Fecha DESC, ALBARANES_COMPRAS.Id DESC, ALBARANES_COMPRAS_LINEAS.Id DESC) AS Compra_Precio
