FROM            TICKETS INNER JOIN
                         TICKETS_LINEAS ON TICKETS.Id = TICKETS_LINEAS.Ticket_Id INNER JOIN
                         ARTICULOS ON TICKETS_LINEAS.CodigoArticulo = ARTICULOS.Codigo LEFT OUTER JOIN
